{
    "name": "brunocfalcao/qrfeedz-authorization",
    "description": "QRFeedz - Authorization rules",
    "keywords": [
        "brunocfalcao",
        "qrfeedz"
    ],
    "homepage": "https://github.com/brunocfalcao/brunocfalcao/qrfeedz-authorization",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Bruno Falc√£o",
            "email": "bruno.c.falcao@brunofalcao.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0",
        "brunocfalcao/qrfeedz-cube": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "QRFeedz\\Authorization\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "QRFeedz\\Authorization\\AuthorizationServiceProvider"
            ]
        }
    }
}
